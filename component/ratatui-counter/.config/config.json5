{
  "keybindings": {
    "Normal": {
      "<q><q>": "Quit", // Quit the application
      "<j>": "ScheduleIncrement",
      "<k>": "ScheduleDecrement",
      "<?>": "ToggleShowHelp",
      "</>": "EnterInsert",
      "<Ctrl-d>": "Quit", // Another way to quit
      "<Ctrl-c>": "Quit", // Yet another way to quit
      "<Ctrl-z>": "Suspend" // Suspend the application
    },
    "Processing": {
      "<j>": "ScheduleIncrement",
      "<k>": "ScheduleDecrement",
    },
    "Help": {
      "<q>": "ToggleShowHelp", // Quit the application
      "<?>": "ToggleShowHelp",
      "<ESC>": "ToggleShowHelp",
    },
  }
}
